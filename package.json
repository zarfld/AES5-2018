{
  "name": "aes5-2018",
  "version": "1.0.0",
  "description": "Open-source, hardware-agnostic implementation of AES5-2018 (Preferred Sampling Frequencies for PCM Audio) providing reference tables, enumerations, and conversion utilities for sample-rate management across AES3, AES11, AES31, AES67, and MILAN stacks.",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "node --test test/index.test.js",
    "test:verbose": "node --test --test-reporter=spec test/index.test.js",
    "example": "node examples/basic-usage.js",
    "example:advanced": "node examples/advanced-usage.js"
  },
  "keywords": [
    "aes5",
    "aes5-2018",
    "audio",
    "pcm",
    "sampling-rate",
    "sample-rate",
    "audio-engineering",
    "aes3",
    "aes11",
    "aes31",
    "aes67",
    "milan",
    "professional-audio",
    "audio-over-ip",
    "dante",
    "avb",
    "tsn",
    "digital-audio"
  ],
